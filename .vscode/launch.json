{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Dbg",
        "skipFiles": ["<node_internals>/**"],
        "program": "${workspaceFolder}/node_modules/vite/bin/vite.js",
        "args": ["dev"],
        "runtimeArgs": ["--inspect-brk"],
        "sourceMaps": true,
        "envFile": "${workspaceFolder}/.env",
        "cwd": "${workspaceFolder}",
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen"
      }
    ]
  }
  
  